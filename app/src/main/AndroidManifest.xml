<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.project2">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:name=".base.BaseApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ui.main.property.addproperty.AddPropertyActivity" />
        <activity android:name=".ui.main.todo.AddToDoActivity" />
        <activity android:name=".ui.main.activities.DocumentActivity" />
        <activity android:name=".ui.main.todo.ToDoActivity" />
        <activity android:name=".ui.main.activities.CollectRentActivity" />
        <activity android:name=".ui.main.activities.TransactionActivity" />
        <activity android:name=".ui.main.activities.TenantActivity" />
        <activity android:name=".ui.main.property.property.PropertyActivity" />
        <activity android:name=".ui.main.home.HomeActivity" />
        <activity android:name=".ui.auth.register.RegisterActivity" />
        <activity android:name=".ui.auth.login.LoginActivity" />
        <activity android:name=".ui.main.home.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>